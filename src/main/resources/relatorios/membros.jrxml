	<queryString>
		<![CDATA[SELECT
     membro.`ID` AS id,
     membro.`NOME` AS membro,
     membro.`CPF` AS CPF,
     membro.`dt_nascimento` AS dt_nascimento,
     templo.`ID` AS id_templo,
     templo.`NOME` AS templo
FROM
     `templo` templo INNER JOIN `membro` membro ON templo.`ID` = membro.`TEMPLO_ID`
ORDER BY
     templo.`ID` ASC,
     membro.`ID` ASC]]>
